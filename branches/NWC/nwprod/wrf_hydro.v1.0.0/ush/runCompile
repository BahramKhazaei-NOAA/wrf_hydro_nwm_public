appDir=/ohd/save/$LOGNAME/chps-wrf-hydro/nwprod/wrf_hydro.v1.0.0
srcDir=/ohd/save/$LOGNAME/chps-wrf-hydro/trunk

echo $appDir
export MODULEPATH=$MODULEPATH:$appDir/modulefiles
module load wrf

rm -f $appDir/exec/*
cd $srcDir
# Run offline configuration file
./configure <<RUNME
8
\r
RUNME

# Offline compiling
./compile_offline_NoahMP.csh

#./compile_offline_Noah.csh

cp -p Run/wrf_hydro.exe $appDir/exec/.
